{
  "name": "full-stack-app",
  "version": "1.0.0",
  "description": "Full stack application",
  "dependencies": {
    "jwt-decode": "^4.0.0",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "start": "npm-run-all --parallel seed start:server",
